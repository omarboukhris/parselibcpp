%import "glib/body.grm"

AXIOM -> file

file -> 
	!imports !classdef

imports ->
	import. s:importfile |
[]

importfile ->  
	lt. path gt. |
	dqu. path dqu.

path -> 
	identifier. |
	identifier. antisl. |
	identifier. headext. |
[]

classdef ->
	classdec. classname=identifier. lcrch. !classbody rcrch. semic. |
	doxy=multilinecomments. classdec. classname=identifier. lcrch. !classbody rcrch. semic. |
[]
